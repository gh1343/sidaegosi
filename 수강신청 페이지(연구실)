<script>
    $(document).ready(function(){
        setInterval(ClassAppendSetInterval , 1000);

        function ClassAppendSetInterval(){
            const rolelist = $(".role_txt li");
            let roleLength = rolelist.length == 0 ? 0 : rolelist.length-1;
            let rolelistActive = $(".role_txt li.active").index();

            rolelist.eq(rolelistActive).removeClass("active");

            if(roleLength <= rolelistActive){
                rolelistActive = 0;
            }else{
                rolelistActive = rolelistActive+1;
            }
            rolelist.eq(rolelistActive).addClass("active");
        }

        var reason_swiper = new Swiper(".reason_slide", {
            slidePerView:1,
            spaceBetween:65,
            loop: true,
            pagination: {
                el: ".reason_nav",
                clickable: true,
                renderBullet: function (index, className) {
                    return '<p class="pull-left num_btn btn_'+(index + 1)+' ' + className + '">0' + (index + 1) + "</p>";
                }
            }, 
            navigation: {
                prevEl: ".reason_slide .btn_prev",
                nextEl: ".reason_slide .btn_next",
            },
            autoplay: {
                delay: 2000,
                disableOnInteraction: false,
            }
        });
        var reason_swiper = new Swiper(".book_slide", {
            slidesPerView: 1,
            effect : 'fade', // 페이드 효과 사용
            fadeEffect: { 
                crossFade: true 
            },
            loop: true,
            navigation: {
                prevEl: ".book_slide .btn_prev",
                nextEl: ".book_slide .btn_next",
            },
            autoplay: {
                delay: 2000,
                disableOnInteraction: false,
            },
            speed : 500
        });
           //도서 리뷰 슬라이드
        var bookreview_swiper = new Swiper(".book_review_slide", {
            loop: true,
            clickable: true,
            navigation: {
                prevEl: ".book_review_slide_wrap .btn_prev",
                nextEl: ".book_review_slide_wrap .btn_next",
            },

            autoplay: {
                delay: 2000,
                disableOnInteraction: false,
            }
        });  
    });  
</script>
